{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 16.13.0"
  },
  "scripts": {
    "start": "tsx watch src/index.ts",
    "ts:watch": "tsc --noEmit --watch",
    "debug": "cross-env DEBUG=colyseus:* tsx watch src/index.ts",
    "loadtest": "tsx loadtest/example.ts --room moveIt --numClients 2",
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000",
    "generateSchema": "schema-codegen src/rooms/Room.ts --output ../client/src/generated-schema/ --ts"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.15.0",
    "@colyseus/testing": "^0.15.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.15.0",
    "@colyseus/playground": "^0.15.3",
    "@colyseus/tools": "^0.15.0",
    "@faker-js/faker": "^8.4.1",
    "@types/victor": "^1.1.5",
    "colyseus": "^0.15.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "victor": "^1.1.0"
  }
}
